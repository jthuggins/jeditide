# Properties file for JExplorer plugin

# general plugin information
plugin.jexplorer.JExplorerPlugin.name=JExplorer
plugin.jexplorer.JExplorerPlugin.author=Amedeo Farello
plugin.jexplorer.JExplorerPlugin.version=0.3.1
plugin.jexplorer.JExplorerPlugin.docs=jexplorer.html

# dependences
plugin.jexplorer.JExplorerPlugin.depend.0=jdk 1.3
plugin.jexplorer.JExplorerPlugin.depend.1=jedit 04.01.99.00
plugin.jexplorer.JExplorerPlugin.depend.2=plugin projectviewer.ProjectPlugin 2.0.2
plugin.jexplorer.JExplorerPlugin.depend.3=plugin errorlist.ErrorListPlugin 1.2.3

# menu labels for actions supplied by dockables.xml
jexplorer-index.label=Index
jexplorer-graph.label=Graph

# menu labels for actions supplied by actions.xml
jexplorer-editModels.label=Models...
jexplorer-selAtCursor.label=Select Item at Cursor
jexplorer-selAtCursorSrcPos.label=Select Item at Cursor (pos. source)

# submenu title in the plugins menu
jexplorer-menu.label=JExplorer

# submenu structure
jexplorer-menu=\
	jexplorer-editModels \
	- \
	jexplorer-index \
	jexplorer-graph \
	- \
	jexplorer-index-float \
	jexplorer-graph-float \
	- \
	jexplorer-selAtCursor \
	jexplorer-selAtCursorSrcPos

# window title
jexplorer-index.title=JExplorer Index
jexplorer-graph.title=JExplorer Graph

# options pane title in global options list
options.jexplorer.label=JExplorer

# Default GLOBAL user properties

# fonts
jexplorer.fontStandard=Default
jexplorer.fontStandardsize=11
jexplorer.fontStandardstyle=0

jexplorer.fontAbstract=Default
jexplorer.fontAbstractsize=11
jexplorer.fontAbstractstyle=2

jexplorer.fontFinal=Default
jexplorer.fontFinalsize=11
jexplorer.fontFinalstyle=1

# colors
jexplorer.colorClasses=\#000000
jexplorer.colorInterfaces=\#3366ff
jexplorer.colorInclusion=\#ff6666
jexplorer.colorFields=\#666666
jexplorer.colorConstructors=\#bb3300
jexplorer.colorMethods=\#006633
# NOT IMPLEMENTED jexplorer.colorOverriding=\#009966

jexplorer.parseOnKeystrokeDelay=1500

# Default MODEL user properties

# elements visualization (common)
jexplorer.useShortNames=false
jexplorer.useShortNames.tooltip=Use local names

jexplorer.showAccessSymbols=true
jexplorer.showAccessSymbols.tooltip=Show UML symbols

# NOT IMPLEMENTED jexplorer.hiliteOverrMethods=true
# NOT IMPLEMENTED jexplorer.hiliteOverrMethods.tooltip=Hilite overriding methods

jexplorer.showDeprecated=true
jexplorer.showDeprecated.tooltip=Show deprecated

jexplorer.pkgcol.tooltip=Package colors...
jexplorer.library.tooltip=Libraries...

jexplorer.colorizePackages=true
jexplorer.colorizePackages.tooltip=Show package colors

# files management
jexplorer.modelStatus.tooltip=Indicates the model status

